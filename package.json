{
  "name": "vue-typescript-bootstrapp",
  "description": "A Vue-TypeScript-Bootstrap Project",
  "version": "1.0.0",
  "author": "Kiho <kihoc22@gmail.com>",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && npm run test && npm run sass && npm run build:parts && npm run clean:css && npm run process:html",
    "build:parts": "npm run sass && npm run compile",
    "build:heroku": "npm run build && npm run copy-h-file && npm run copy-h-vendor",
    "ci:teamcity": "karma --env=tc start karma.coverage.js && npm run coverage:remap",
    "ci:jenkins": "karma --env=jk start karma.coverage.js && npm run coverage:remap",
    "clean": "rimraf dist && rimraf coverage",
    "clean:css": "cleancss -o dist/css/main.min.css src/css/main.css",
    "compile": "webpack --config config/webpack.config.prod.js",
    "coverage": "npm run coverage:run && npm run coverage:remap && npm run coverage:open",
    "coverage:open": "opn coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development karma start karma.coverage.js",
    "copy-vendor": "cpx src/vendor/**/* ./dist/vendor",
    "copy-h-file": "cpx ./dist/**/* ../heroku/vue-app/public",
    "copy-h-vendor": "cpx src/vendor/toastr/* ../heroku/vue-app/public/vendor/toastr",
    "dev": "cross-env NODE_ENV=development npm run sass && concurrently --kill-others \"npm run serve-api\" \"webpack-dev-server --hot --inline\" \"node-sass --watch --output ./src/css ./src/sass\"",
    "lint": "tslint src/**/*.ts",
    "process:html": "htmlprocessor src/index.html -o dist/index.html",
    "sass": "node-sass --output ./src/css ./src/sass",
    "serve": "http-server dist/ -o",
    "test": "cross-env NODE_ENV=development karma start karma.unit.js",
    "test:debug": "cross-env NODE_ENV=development karma start karma.debug.js",
    "test:watch": "cross-env NODE_ENV=development karma start karma.unit.js --singleRun=false --auto-watch",
    "serve-api": "node server/index.js"
  },
  "dependencies": {
    "axios": "~0.15.3",
    "bootstrap": "~3.3.7",
    "fuse.js": "^3.2.0",
    "vue": "^2.2.4",
    "vue-class-component": "^5.0.0",
    "vue-property-decorator": "^4.0.0",
    "vue-router": "^2.3.0"
  },
  "devDependencies": {
    "@types/datatables.net": "^1.10.1",
    "@types/jquery": "^2.0.41",
    "@types/mocha": "~2.2.40",
    "@types/node": "~7.0.8",
    "@types/sinon": "~1.16.36",
    "@types/toastr": "^2.1.33",
    "awesome-typescript-loader": "~3.1.2",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.5",
    "babel-resolver": "^0.0.18",
    "body-parser": "^1.15.2",
    "chai": "~3.5.0",
    "clean-css-cli": "~4.0.9",
    "compression-webpack-plugin": "~0.3.2",
    "concurrently": "~3.4.0",
    "copy-webpack-plugin": "~4.0.1",
    "copyfiles": "^1.2.0",
    "cpx": "^1.5.0",
    "cross-env": "~3.2.4",
    "css-loader": "^0.28.4",
    "es6-promise": "~4.1.0",
    "express": "^4.14.0",
    "file-loader": "~0.10.1",
    "globby": "~6.1.0",
    "htmlprocessor": "~0.2.4",
    "http-server": "~0.9.0",
    "istanbul-instrumenter-loader": "~2.0.0",
    "json-loader": "~0.5.4",
    "karma": "~1.5.0",
    "karma-chai": "~0.1.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coverage": "~1.1.1",
    "karma-junit-reporter": "~1.2.0",
    "karma-mocha": "~1.3.0",
    "karma-mocha-reporter": "~2.2.2",
    "karma-phantomjs-launcher": "~1.0.4",
    "karma-sinon": "~1.0.5",
    "karma-source-map-support": "~1.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-teamcity-reporter": "~1.0.0",
    "karma-webpack": "~2.0.3",
    "lodash.merge": "~4.6.0",
    "material-design-icons": "^3.0.1",
    "materialize-css": "^0.98.2",
    "minimist": "~1.2.0",
    "mocha": "~3.2.0",
    "ncp": "~2.0.0",
    "node-sass": "~4.5.0",
    "opn-cli": "~3.1.0",
    "phantomjs-prebuilt": "~2.1.14",
    "raw-loader": "~0.5.1",
    "remap-istanbul": "~0.9.1",
    "rimraf": "~2.6.1",
    "route-parser": "^0.0.5",
    "sinon": "~2.1.0",
    "tslint": "~4.5.1",
    "tslint-loader": "~3.4.3",
    "typescript": "^2.3.0",
    "vue-loader": "^12.2.1",
    "vue-progressbar": "^0.7.2",
    "vue-template-compiler": "^2.3.4",
    "webpack": "~2.2.1",
    "webpack-dev-server": "~2.4.2"
  },
  "babelConfig": {
    "server": {
      "presets": [
        "es2015",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators-legacy"
      ]
    }
  },
  "config": {
    "assetsPublicPath": "/assets/"
  }
}
